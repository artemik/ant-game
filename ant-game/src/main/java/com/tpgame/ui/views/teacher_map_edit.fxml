<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane fx:id="gridPane" prefHeight="428.0" prefWidth="788.0" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/8"
          xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="70.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints fillHeight="true" minHeight="10.0" percentHeight="100" valignment="CENTER" vgrow="ALWAYS"/>
    </rowConstraints>
    <children>
        <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: #f5f5f5;" GridPane.columnIndex="1">
            <children>
                <Label text="Map size:">
                    <VBox.margin>
                        <Insets top="10.0"/>
                    </VBox.margin>
                </Label>
                <HBox prefHeight="40.0" prefWidth="270.0">
                    <children>
                        <Label text="Width:">
                            <HBox.margin>
                                <Insets left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                        </Label>
                        <ComboBox fx:id="widthComboBox" prefHeight="25.0" prefWidth="55.0" promptText="15">
                            <HBox.margin>
                                <Insets right="10.0" top="10.0"/>
                            </HBox.margin>
                        </ComboBox>
                        <Label text="Height:">
                            <HBox.margin>
                                <Insets right="10.0" top="10.0"/>
                            </HBox.margin>
                        </Label>
                        <ComboBox fx:id="heightComboBox" prefHeight="25.0" prefWidth="55.0" promptText="15">
                            <HBox.margin>
                                <Insets right="10.0" top="10.0"/>
                            </HBox.margin>
                        </ComboBox>
                    </children>
                </HBox>
                <Label text="Tool:">
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                </Label>
                <HBox prefWidth="200.0">
                    <children>
                        <ListView fx:id="toolsList" prefHeight="120">
                            <HBox.margin>
                                <Insets left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                        </ListView>
                    </children>
                </HBox>
            </children>
            <GridPane.margin>
                <Insets/>
            </GridPane.margin>
            <padding>
                <Insets left="10.0"/>
            </padding>
        </VBox>
    </children>
</GridPane>
