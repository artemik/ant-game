<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane fx:id="borderPane" prefHeight="336.0" prefWidth="276.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <Pane prefHeight="336.0" prefWidth="269.0">
            <children>
                <GridPane hgap="10.0" layoutX="11.0" layoutY="13.0" prefHeight="298.0" prefWidth="205.0" vgap="10.0"
                          BorderPane.alignment="CENTER">
                    <columnConstraints>
                        <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" percentWidth="45.0"
                                           prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Label text="Width:" GridPane.rowIndex="1"/>
                        <Label text="Cubes:" GridPane.rowIndex="2"/>
                        <ComboBox fx:id="widthBox" prefHeight="25.0" prefWidth="162.0" GridPane.columnIndex="1"
                                  GridPane.rowIndex="1"/>
                        <HBox alignment="CENTER_RIGHT" prefHeight="66.0" prefWidth="370.0" GridPane.columnIndex="1"
                              GridPane.rowIndex="5">
                            <children>
                                <Button fx:id="cancelBtn" mnemonicParsing="false" text="CANCEL">
                                    <HBox.margin>
                                        <Insets right="10.0"/>
                                    </HBox.margin>
                                </Button>
                                <Button fx:id="okBtn" mnemonicParsing="false" text="OK"/>
                            </children>
                        </HBox>
                        <Label text="Height:"/>
                        <ComboBox fx:id="heightBox" prefHeight="25.0" prefWidth="128.0" GridPane.columnIndex="1"/>
                        <ComboBox fx:id="cubesBox" prefHeight="25.0" prefWidth="186.0" GridPane.columnIndex="1"
                                  GridPane.rowIndex="2"/>
                        <Label text="Holes:" GridPane.rowIndex="3"/>
                        <ComboBox fx:id="holesBox" prefHeight="25.0" prefWidth="234.0" GridPane.columnIndex="1"
                                  GridPane.rowIndex="3"/>
                        <Label text="Occupied cells:" GridPane.rowIndex="4"/>
                        <ComboBox fx:id="occupiedBox" prefHeight="25.0" prefWidth="215.0" GridPane.columnIndex="1"
                                  GridPane.rowIndex="4"/>
                    </children>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                </GridPane>
            </children>
        </Pane>
    </center>
</BorderPane>
